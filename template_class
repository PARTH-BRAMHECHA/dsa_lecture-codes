#include <iostream>

using namespace std;
template<class T>
class cal{
    private:T a;
            T b;
    
    public:cal(T x,T y){
            a=x;
            b=y;
    }
            T addition(){
                return(a+b);
    }
};
int main()
{
    cal<int> obj(1,2);
    cout<<obj.addition();

    return 0;
}
